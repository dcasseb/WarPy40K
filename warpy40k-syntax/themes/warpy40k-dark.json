      "settings": {
        "foreground": "#c586c0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "WarPy40K Variables",
      "scope": "variable.other.warpy40k",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "WarPy40K Comments",
      "scope": "comment.line.number-sign.warpy40k",
